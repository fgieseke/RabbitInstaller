{
  "exchangeName": "Translator-In",
  "consumer": [
    {
      "executable": "simulation",
      "routingKeys":  "raw.text.xyz.#" ,
      "message": "Origin 'xyz' not supported."
    }
  ],
  "router": {
    "executable": "simulation",
    "exchangeName": "Translator-Out",
    "routes": [
      {
        "queueName": "Translator-{origin}-Queue",
        "routingKey": "raw.text.abc.#",
        "to": "doc.*.*.created"
      },
      {
        "queueName": "Translator-{origin}-Queue",
        "routingKey": "raw.text.dpa.#",
        "to": "doc.*.*.created"
      },
      {
        "queueName": "Translator-{contentType}-Queue",
        "routingKey": "raw.vpms.create",
        "to": "doc.*.*.created"
      },
      {
        "queueName": "Translator-{contentType}-Queue",
        "routingKey": "raw.archive.create",
        "to": "doc.*.*.created"
      },
      {
        "queueName": "TranslatorRemove-{contentType}-Queue",
        "routingKey": "raw.archive.remove" ,
        "to": "doc.*.*.deleted"
      },
      {
        "queueName": "TranslatorRemove-{contentType}-Queue",
        "routingKey": "raw.vpms.remove",
        "to": "doc.*.*.deleted"
      }
    ]
  }
}
