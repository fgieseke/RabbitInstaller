{
  "hosts": [ "localhost" ],
  "user": "guest",
  "password": "guest",
  "vhost": "/",
  "desc": "Experimentelle Einstellungen für die Docker-Queue",
  "executableBasePath": "c:\\",
  "exchanges": [
    {
      "exchangeName": "Service",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "binding": {}
    },
    {
      "exchangeName": "Emitter-Out",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "Translator-In",
            "routingKeys": [ "raw.#" ]
          }
        ]
      }
    },
    {
      "exchangeName": "Translator-In",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "autoDelete": false,
      "binding": {}
    },
    {
      "exchangeName": "Translator-Out",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "IndexerGateway",
            "routingKeys": [ "doc.#" ]
          },
          {
            "exchangeName": "EnricherCache-In",
            "routingKeys": [ "doc.*.*.delete", "doc.*.*.create", "doc.*.*.serial" ]
          }
        ]
      }
    },
    {
      "exchangeName": "Enricher-In",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "binding": {}
    },
    {
      "exchangeName": "Enricher-Out",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "Indexer-In",
            "routingKeys": [ "partial.*.*.enrich" ]
          },
          {
            "exchangeName": "EnricherCache-In",
            "routingKeys": [ "partial.*.*.enrich" ]
          }
        ]
      }
    },
    {
      "exchangeName": "Indexer-In",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "autoDelete": false,
      "binding": {}
    },
    {
      "exchangeName": "Indexer-Out",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "Enricher-In",
            "routingKeys": [ "doc.*.*.indexed" ]
          }
        ]
      }
    },
    {
      "exchangeName": "EnricherCache-In",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "autoDelete": false,
      "binding": {}
    },
    {
      "exchangeName": "EnricherCache-Out",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "Indexer-In",
            "routingKeys": [ "doc.*.*.refresh" ]
          }
        ]
      }
    }
  ]
}
