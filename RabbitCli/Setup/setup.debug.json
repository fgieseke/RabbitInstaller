{
  "desc": "Experimentelle Einstellungen für die Docker-Queue",
  "executableBasePath": "d:\\",
  "exchanges": [
    {
      "exchangeName": "System",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "binding": {}
    },
    {
      "exchangeName": "Emitter-Out",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "Translator-In",
            "routingKeys": [ "raw.#" ]
          }
        ]
      }
    },
    {
      "exchangeName": "Translator-In",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "autoDelete": false
    },
    {
      "exchangeName": "Translator-Out",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "EnricherCache-In",
            "routingKeys": [ "doc.*.*.deleted", "doc.*.*.created" ]
          }
        ]
      }
    },
    {
      "exchangeName": "EnricherCache-In",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "autoDelete": false
    },
    {
      "exchangeName": "EnricherCache-Out",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "IndexerGateway",
            "routingKeys": [ "doc.*.*.refreshed" ]
          }
        ]
      }
    },
    // Legacy-Exchanges:: Will not be deleted on cleanup
    {
      "exchangeName": "DataGateway",
      "exchangeType": "Topic",
      "direction": "out",
      "durable": true,
      "autoDelete": false,
      "isLegacy": true,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "EnricherCache-In",
            "routingKeys": [ "doc.*.#.index.update", "doc.*.#.index.delete", "doc.*.#.index.create" ]
          },
          {
            "exchangeName": "IndexerGateway",
            "routingKeys": [ "doc.*.#.index.update", "doc.*.#.index.delete" ]
          }
        ]
      }
    },
    {
      "exchangeName": "IndexerGateway",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "autoDelete": false,
      "isLegacy": true,
      "binding": {
        "exchanges": [
          {
            "exchangeName": "IndexerHavarieGateway",
            "routingKeys": [ "doc.*.*.refreshed", "doc.*.#.index.*" ]
          },
          {
            "exchangeName": "IndexerProdGateway",
            "routingKeys": [ "doc.*.*.refreshed", "doc.*.#.index.*" ]
          },
          {
            "exchangeName": "EnricherGateway",
            "routingKeys": [ "doc.*.enrich", "doc.*.enrich.#" ]
          }
        ]
      }
    },
    {
      "exchangeName": "IndexerHavarieGateway",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "autoDelete": false,
      "isLegacy": true
    },
    {
      "exchangeName": "IndexerProdGateway",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "autoDelete": false,
      "isLegacy": true
    },
    {
      "exchangeName": "EnricherGateway",
      "exchangeType": "Topic",
      "direction": "in",
      "durable": true,
      "autoDelete": false,
      "isLegacy": true
    }

  ]
}
