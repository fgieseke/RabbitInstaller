{
  "scenarios": [
    {
      "name": "agency-create",
      "environment": [ "Translator:Text"],
      "emitter": {
        "exchangeName": "Emitter",
        "routingKey": "raw.text.abc.create"
      },
      "routers": ["Translator:create"]
    },
    {
      "name": "VideoBulk",
      "environment": "Video",
      "publishers": [
        {
          "name": "Emitter.Newsroom-Bulk",
          "exchangeName": "Emitter",
          "routingKey": "raw.video.newsroom.bulk",
          "messageCount": 1,
          "period": 1000
        }
      ]
    },
    {
      "name": "VideoCreate",
      "publishers": [
        {
          "name": "Emitter.Newsroom-Create",
          "exchangeName": "Emitter",
          "routingKey": "raw.video.newsroom.create",
          "messageCount": 1,
          "period": 1000
        }
      ]
    },
    {
      "name": "VideoDelete",
      "publishers": [
        {
          "name": "Emitter.Newsroom-Delete",
          "exchangeName": "Emitter",
          "routingKey": "raw.video.newsroom.delete",
          "messageCount": 1,
          "period": 1000
        }
      ]
    }

  ]
}



