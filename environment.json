{
  "environments": [
    {
      "exchangeName": "Translator",
      "queueName": "Translator-{contentType}-{origin}-Queue",
      "variants": [
        {
          "name": "Text",
          "consumer": 
            {
              "binding": {
                "routingKeys": [ "raw.text.xyz.create" ]
              }
            }
          ,
          "router": 
            {
              "binding": {
                "routingKeys": [ "raw.text.abc.create", "raw.text.dpa.create" ]
              },
              "publish": {
                "exchangeName": "Document",
                "modes": [
                  {
                    "name": "create",
                    "routingKey": "doc.*.*.create"
                  },
                  {
                    "name": "delete",
                    "RoutingKey": "doc.*.*.delete"
                  }
                ]
              }
            }
          
        }
      ]
    },
    {
      "exchangeName": "Enricher",
      "queueName": "Enricher-{contentType}-{origin}-Queue",
      "variants": [
        {
          "name": "KitKeyword",
          "routers": [
            {
              "binding": {
                "routingKeys": [ "doc.*.*.indexed" ],
                "documentType": "video",
                "origins": [ "KitKeyword" ]
              }
            }
          ],
          "modes": [
            {
              "name": "enrich",
              "exchangeName": "PartialDocument",
              "RoutingKey": "partial.*.*.enrich"
            }
          ]
        }
      ]
    }
  ]
}